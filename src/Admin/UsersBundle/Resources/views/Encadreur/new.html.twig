                    <div class="bg-danger">{{ form_errors(form.nomEncadreur) }}</div>
                    <div class="form-group input-group">
                        {{ form_widget(form.nomEncadreur,  {'attr': {'placeholder':'nomEncadreur', 'title':'nomEncadreur', 'class':'form-control'}}) }}
                    </div>
                </div>
                <div class="col-sm-12">
