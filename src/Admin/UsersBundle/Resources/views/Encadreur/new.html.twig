
{% block body %}
    <script>
        $(document).ready(function() {
            $('#ajaxFormE').on('submit', function(e) {
                e.preventDefault();
                $.ajax({
                    url:$(this).attr('action'),
                    type:$(this).attr('method'),
                    data: $(this).serialize(),
                    beforeSend: function(){
                        $('#orderModalcontent').html("waiting .............");
                    }, success: function() {
                        //$('#orderModal').hide();
                        alert("succes");
                    },
                    complete: function(){
                        alert("comple");
                        $('#orderModal').hide();

                    }
                });

            });

        });

    </script>
    <div  id="tab2">
        <div class="form_error"></div>
        <form method="POST" id="ajaxFormE" action="{{path('GestionUsersEn_create')}}" {{ form_enctype(form) }}>
