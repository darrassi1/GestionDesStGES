{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Specialite list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Idspecialite</th>
                <th>Specialite</th>
                <th>Diplome</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('Specialite_show', { 'id': entity.idSpecialite }) }}">{{ entity.idSpecialite }}</a></td>
                <td>{{ entity.specialite }}</td>
                <td>{{ entity.diplome }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('Specialite_show', { 'id': entity.idSpecialite }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('Specialite_edit', { 'id': entity.idSpecialite }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('Specialite_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
